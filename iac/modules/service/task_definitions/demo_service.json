[
  {
    "Name": "demo-service",
    "Essential": true,
    "Image": "${ecs_image}",
    "Memory": 256,
    "PortMappings": [
      {
        "ContainerPort": 8080,
        "HostPort": 8080
      }
    ],
    "LogConfiguration": {
      "LogDriver": "awslogs",
      "Options": {
        "awslogs-group": "${cloudwatch_logs_group}",
        "awslogs-region": "${aws_region}"
      }
    }
  }
]